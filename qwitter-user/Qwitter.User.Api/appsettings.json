{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Audience": "Qwitter",
  "Issuer": "qwitter.com",
  "AllowedHosts": "*",
  "service-hosts": {
    "qwitter-user-service": "https://localhost:5201"
  },
  "certificates": {
    "root_ca_path": "certificates/root_ca/root_cert.pem",
    "jwt_cert_path": "certificates/jwt/jwt_cert.pem",
    "client_cert_path": "certificates/user/user_api_cert.pfx",
    "client_cert_password": "password"
  }
}